</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;plugins/docker-compose&#34;</span><span class="p">,</span> <span class="n">from</span><span class="p">:</span><span class="n">oh</span><span class="o">-</span><span class="n">my</span><span class="o">-</span><span class="n">zsh</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;b4b4r07/enhancd&#34;</span><span class="p">,</span> <span class="n">use</span><span class="p">:</span><span class="n">init</span><span class="o">.</span><span class="n">sh</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;tarrasch/zsh-bd&#34;</span><span class="p">,</span> <span class="n">at</span><span class="p">:</span><span class="mi">3724233</span><span class="p">,</span> <span class="n">frozen</span><span class="p">:</span><span class="mi">1</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;jhawthorn/fzy&#34;</span><span class="p">,</span> \
</span></span><span class="line"><span class="cl">    <span class="n">as</span><span class="p">:</span><span class="n">command</span><span class="p">,</span> \
</span></span><span class="line"><span class="cl">    <span class="n">rename</span><span class="o">-</span><span class="n">to</span><span class="p">:</span><span class="n">fzy</span><span class="p">,</span> \
</span></span><span class="line"><span class="cl">    <span class="n">hook</span><span class="o">-</span><span class="n">build</span><span class="p">:</span><span class="s2">&#34;</span>
</span></span><span class="line"><span class="cl">    <span class="p">{</span>
</span></span><span class="line"><span class="cl">        <span class="n">make</span>
</span></span><span class="line"><span class="cl">        <span class="n">sudo</span> <span class="n">make</span> <span class="n">install</span>
</span></span><span class="line"><span class="cl">    <span class="p">}</span> <span class="o">&amp;&gt;/</span><span class="n">dev</span><span class="o">/</span><span class="n">null</span>
</span></span><span class="line"><span class="cl">    <span class="s2">&#34;</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;junegunn/fzf-bin&#34;</span><span class="p">,</span> <span class="n">as</span><span class="p">:</span><span class="n">command</span><span class="p">,</span> <span class="n">from</span><span class="p">:</span><span class="n">gh</span><span class="o">-</span><span class="n">r</span><span class="p">,</span> <span class="n">rename</span><span class="o">-</span><span class="n">to</span><span class="p">:</span><span class="n">fzf</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="c1"># ついでに tmux 用の拡張も入れるといい</span>
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="s2">&#34;junegunn/fzf&#34;</span><span class="p">,</span> <span class="n">as</span><span class="p">:</span><span class="n">command</span><span class="p">,</span> <span class="n">use</span><span class="p">:</span><span class="n">bin</span><span class="o">/</span><span class="n">fzf</span><span class="o">-</span><span class="n">tmux</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="k">if</span> <span class="o">!</span> <span class="n">zplug</span> <span class="n">check</span> <span class="o">--</span><span class="n">verbose</span><span class="p">;</span> <span class="n">then</span>
</span></span><span class="line"><span class="cl">    <span class="n">printf</span> <span class="s2">&#34;Install? [y/N]: &#34;</span>
</span></span><span class="line"><span class="cl">    <span class="k">if</span> <span class="n">read</span> <span class="o">-</span><span class="n">q</span><span class="p">;</span> <span class="n">then</span>
</span></span><span class="line"><span class="cl">        <span class="n">echo</span><span class="p">;</span> <span class="n">zplug</span> <span class="n">install</span>
</span></span><span class="line"><span class="cl">    <span class="n">fi</span>
</span></span><span class="line"><span class="cl"><span class="n">fi</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="c1">#沒有用</span>
</span></span><span class="line"><span class="cl"><span class="c1">#POWERLEVEL9K_MODE=&#34;Hack&#34;</span>
</span></span><span class="line"><span class="cl">
</span></span><span class="line"><span class="cl"><span class="n">zplug</span> <span class="nb">load</span>
</span></span></code></pre></td></tr></table>
